{
  "name": "charonlab/testing",
  "description": "Testing utilities for charonlab packages.",
  "license": "MIT",
  "type": "library",
  "require": {
    "php": ">=8.1",
    "phpbench/phpbench": "^1.2",
    "phpunit/phpunit": "^11.0"
  },
  "autoload": {
    "psr-4": {
      "Charon\\Testing\\": "src/"
    }
  },
  "config": {
    "optimize-autoloader": true,
    "platform": {
      "php": "8.2.16"
    },
    "sort-packages": true
  }
}
